{
	"name": "frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"lint": "eslint \"src/**/*.{js,svelte}\"",
		"lint:fix": "eslint --fix \"src/**/*.{js,svelte}\""
	},
