		"publint": "^0.3.15",
		"svelte": "^5.45.6",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"eslint": "^9.39.2",
		"flatpickr": "^4.6.13",
		"pino": "^10.2.0",
		"pino-pretty": "^13.1.3",
		"tailwindcss": "^4.1.18"
